* {
    margin: 0;
    padding: 0;
    font-family: "微软雅黑"
}

a {
    text-decoration: none;
    height: 100%;
    width: 100%;
}

li {
    list-style: none
}

body,
html {
    width: 100%;
    height: 100%;
    position: relative;
    overflow: hidden;
    background: #f5f4f7
}

.none {
    display: none
}

.none_visibility {
    visibility: hidden
}


/* header */


/* 公共header */

header {
    position: fixed;
    z-index: 999;
    left: 0;
    top: 0;
    width: 100%;
    height: 0.9rem;
    font-size: 0.32rem;
    padding: 0rem 0.3rem;
    /* border-bottom: 1px solid #eee; */
    box-sizing: border-box;
    line-height: 0.9rem;
    text-align: center;
    background: #fff;
    border-bottom: 1px solid #eee;
    box-sizing: border-box;
}

.back {
    display: block;
    width: 1rem;
    height: 100%;
    position: absolute;
    left: 0;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.back img {
    width: 0.14rem;
    height: 0.27rem;
}

.header_cen {
    color: #1f1212;
}


/* 内容 */

.con {
    width: 100%;
    position: fixed;
    top: 0.9rem;
    left: 0;
    bottom: 1rem;
    overflow: auto;
    /* background: #fff; */
}


/* 高度一直延伸到底部 */

.nobottom {
    bottom: 0 !important;
}


/* swiper */

.swiper-slide {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.swiper_box {
    width: 100%;
    height: 100%;
    overflow: visible;
}

.nav_a {
    width: 20%;
    height: 100%;
    float: left;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.nav_a img {
    width: 0.4rem;
    height: 0.4rem;
    margin-bottom: 0.1rem;
}

.nav_a p {
    color: #333333;
    font-size: 0.24rem;
}


/* 灰色间隔 */
.interval {
    width: 100%;
    height: 0.1rem;
    background: #f5f5f7;
}


/* 底线 */

.baseline {
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    padding: 0.5rem 0rem;
    background: #f5f5f7;
}

.baseline span {
    display: block;
    width: 30%;
    height: 1px;
    background: #e3e3e3;
}

.baseline p {
    color: #999999;
    font-size: 0.2rem;
    margin: 0rem 0.25rem;
}


/* 直播样式 */

.list {
    float: left;
    width: 49%;
    position: relative;
    font-size: 0;
    margin: 0rem 0.5%;
    margin-bottom: 0.2rem;
}

.list_a {
    display: block;
    position: relative;
}

.list_img {
    width: 100%;
    height: 2.4rem;
    margin-bottom: 0.05rem;
}

.list_name {
    color: #333333;
    font-size: 0.28rem;
    margin-bottom: 0.05rem;
    padding-left: 0.1rem;
}

.list_p {
    padding-left: 0.1rem;
    color: #666666;
    font-size: 0.24rem;
    width: 60%;
}

.list_name,
.list_p {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}


/* 标签 */

.list_label {
    position: absolute;
    color: #fff;
    font-size: 0.18rem;
    background: #82a541;
    border-top-right-radius: 0.35rem;
    border-bottom-right-radius: 0.35rem;
    border-top-left-radius: 0.5rem;
    padding: 0.02rem 0.1rem;
    bottom: 0;
    right: 0.15rem;
}


/* 首页nav 的4个子页面的顶部图片  */


/* 顶部图片 */

.top_img {
    width: 100%;
    height: 2.6rem;
    font-size: 0;
}

.top_img img {
    width: 100%;
    height: 100%;
}


/* 商品  大图 和 列表 切换  */


/* 顶部功能盒子，好切换  position: fixed;*/

.calssify_top_function {
    width: 100%;
    /* overflow: hidden; */
    position: fixed;
    z-index: 99999;
    top: 0.9rem;
    left: 0;
}


/* 筛选 切换模式*/

.screen_box {
    width: 100%;
    height: 0.7rem;
    padding: 0rem 0.2rem;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    background: #fff;
    position: relative;
}


/* 筛选 */

.screen,
.pattern_box {
    width: 49.8%;
    height: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.screen_p {
    color: #7ea83a;
    font-size: 0.28rem;
    margin-right: 0.1rem;
}

.screen_img {
    width: 0.17rem;
    height: 0.25rem;
}


/* 筛选下拉 */

.screen_down {
    background: #fff;
    width: 100%;
    margin: 0rem -0.2rem;
    box-sizing: border-box;
    position: absolute;
    z-index: 99;
    top: 100%;
}

.screen_down_p {
    color: #333333;
    font-size: 0.3rem;
    line-height: 0.8rem;
    padding-left: 0.2rem;
    box-sizing: border-box;
    border-bottom: 1px solid #eee;
    box-sizing: border-box;
}

.screen_down_p:last-child {
    border: none;
}


/* 分隔线 */

.screen_xian {
    display: block;
    width: 0.4%;
    height: 0.4rem;
    background: #eee;
}


/* 模式 */

.pattern {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.pattern_p {
    color: #999999;
    font-size: 0.28rem;
    margin-right: 0.1rem;
}


/* 列表 */

.column_img,
.big_img {
    width: 0.27rem;
    height: 0.27rem;
}


/* 商品 */


/* 公共 */

.shop_con {
    width: 100%;
    position: relative;
}

.shop_box {
    background: #fff;
    position: relative;
}

.shop_a {
    display: block;
    /* padding: 0rem 0rem 0.2rem 0rem; */
    box-sizing: border-box;
    position: relative;
}

.shop_img {
    /* padding: 0.2rem; */
    box-sizing: border-box;
}

.shop_img img {
    width: 100%;
    height: 100%;
}

.shop_name {
    color: #333333;
    font-size: 0.32rem;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.shop_money {
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: flex-end;
}

.json_shop_img {
    position: absolute;
}


/* 大图的类 */

.shop_con_big {
    padding: 0.2rem;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    flex-wrap: wrap;
}

.shop_box_big {
    width: 48.5%;
    margin-bottom: 0.2rem;
    border-radius: 3px;
    overflow: hidden;
}

.shop_a_big {
    display: block;
}

.shop_img_big {
    width: 100%;
    height: 3.4rem !important;
}

.bot_box_big {
    width: 100%;
    padding: 0.1rem 0.15rem 0.2rem 0.15rem;
    box-sizing: border-box;
}

.shop_name_big {
    margin-bottom: 0.3rem;
}

.json_shop_img_big {
    right: 0.25rem;
    bottom: 0.2rem;
}


/* 列表的类 */

.shop_con_list {
    padding: 0rem 0.2rem;
    margin: 0.1rem 0rem;
    box-sizing: border-box;
    background: #fff;
}

.shop_box_list {
    width: 100%;
    height: 2.9rem;
    border-bottom: 1px solid #eee;
    box-sizing: border-box;
}

.shop_box_list:last-child {
    border-bottom: none;
}

.shop_a_list {
    display: flex;
    flex-direction: row;
    padding: 0.3rem 0rem;
}

.shop_img_list {
    width: 30% !important;
    /* border-radius: 4px; */
    overflow: hidden;
    /* height: 2.9rem;
    padding: 0.3rem 0rem;
    box-sizing: border-box; */
}

.bot_box_list {
    width: 70%;
    padding: 0.2rem 0.1rem 0rem 0.2rem;
    box-sizing: border-box;
}

.shop_name_list {
    margin-bottom: 0.1rem;
}

.shop_p {
    width: 100%;
    height: 1rem;
}

.shop_p p {
    color: #666666;
    font-size: 0.28rem;
    display: -webkit-box;
    /*! autoprefixer: ignore next */
    -webkit-box-orient: vertical;
    /* autoprefixer: on */
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.json_shop_img_list {
    right: 0.3rem;
    bottom: 0.6rem;
}


/* 加入购物车  icon */

.json_shop_img {
    width: 0.48rem;
    height: 0.48rem;
}


/* 现价  原价 */

.new_money {
    color: #ed2018;
    font-size: 0.32rem;
    margin-right: 0.15rem;
    /* font-weight: bold; */
}

.old_money {
    color: #666666;
    font-size: 0.24rem;
    text-decoration: line-through;
}


/* 商品列表  样式 */


/* 商品 */

.Recommend_shop {
    width: 100%;
    /* height: 2.5rem; */
    height: 2.9rem;
    overflow: hidden;
    border-bottom: 1px solid #eee;
    box-sizing: border-box;
}

.Recommend_shop_a {
    display: block;
    padding: 0.3rem 0rem;
    box-sizing: border-box;
}


/* 左边 */

.Recommend_shop_left {
    width: 30%;
    height: 100%;
    float: left;
    /* padding: 0.1rem; */
    /* border-radius: 4px; */
    box-sizing: border-box;
    overflow: hidden;
}

.Recommend_shop_right {
    width: 70%;
    height: 100%;
    float: left;
    /* padding: 0.2rem 0.1rem 0.2rem 0.2rem; */
    padding: 0.2rem 0.1rem 0rem 0.2rem;
    /* padding-top: 0.3rem; */
    box-sizing: border-box;
    position: relative;
}

.Recommend_shop_left img {
    width: 100%;
    height: 100%;
}

.Recommend_shop_name {
    color: #333333;
    font-size: 0.32rem;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-bottom: 0.08rem;
}

.Recommend_shop_p {
    width: 100%;
    height: 0.8rem;
    margin-bottom: 0.1rem;
}

.Recommend_shop_p p {
    color: #666666;
    font-size: 0.28rem;
    display: -webkit-box;
    /*! autoprefixer: ignore next */
    -webkit-box-orient: vertical;
    /* autoprefixer: on */
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.recommend_money {
    font-size: 0;
}


/* .Recommend_shop_money{
    color: #e10707;
    font-size: 0.36rem;
    margin-top: 0.25rem;
} */

.Recommend_shop_img {
    position: absolute;
    right: 0.2rem;
    bottom: 0.2rem;
}


/* 商品  加减 */


/* 加减 */

.plus_reduce {
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    align-items: center;
}

.plus,
.reduce {
    width: 0.35rem;
    height: 0.35rem;
}

.shop_number {
    width: 0.6rem;
    height: 0.35rem;
    margin: 0rem 0.1rem;
    text-align: center;
    line-height: 0.35rem;
    border: none;
}


/* 基地信息 */


/* 商品列表 */

.slide_list {
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.slide_list_a {
    display: block;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding-bottom: 0.2rem;
}


/* 查看更多 */

.more_slide {
    height: auto !important;
}

.more_a_img {
    width: 0.6rem;
    height: 0.35rem;
    margin-bottom: 0.1rem;
}

.more_a_p {
    color: #333333;
    font-size: 0.32rem;
}

.base_shop_swiper {
    padding-bottom: 0.15rem;
}

.slide_img_box {
    width: 100%;
    height: 2rem;
    border-radius: 5px;
    overflow: hidden;
    position: relative;
    margin-bottom: 0.15rem;
}

.slide_img {
    width: 100%;
    height: 100%;
}

.slide_img_p {
    position: absolute;
    display: block;
    bottom: 0rem;
    left: 0rem;
    color: #fff;
    font-size: 0.18rem;
    padding: 0.03rem 0.1rem;
    /* padding: 0.5rem; */
    background: #82a541;
    border-bottom-right-radius: 0.5rem;
    border-top-right-radius: 0.5rem;
    border-top-left-radius: 0.6rem;
}

.slide_title {
    color: #333333;
    font-size: 0.28rem;
    margin-bottom: 0.07rem;
}

.slide_p {
    color: #666666;
    font-size: 0.24rem;
    margin-bottom: 0.1rem;
}

.slide_money {
    color: #ed2018;
    font-size: 0.28rem;
    /* font-weight: bold; */
}

.slide_title,
.slide_p,
.slide_money {
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}


/* 视频样式和按钮安阳 */

.broadcast_video {
    width: 100%;
    height: 100%;
}

.video_play {
    width: 1.16rem;
    height: 1.16rem;
    border-radius: 0.5rem;
    overflow: hidden;
    background: rgba(0, 0, 0, 0.5);
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    position: absolute;
    font-size: 0;
    top: 50%;
    margin-top: -0.58rem;
    left: 50%;
    margin-left: -0.58rem;
}

.video_play img {
    width: 0.5rem;
    height: 0.66rem;
    margin-left: 0.1rem;
}


/* 关注 */


/* 基地信息 */

.base_information_box {
    width: 100%;
    padding: 0.1rem 0.4rem 0.25rem 0.3rem;
    border-bottom: 1px solid #eee;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.base_information_box_left {
    height: 100%;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
}


/* 头像 */

.base_portrait {
    width: 1rem;
    height: 1rem;
    border-radius: 100%;
    overflow: hidden;
    float: left;
    margin-right: 0.15rem;
    font-size: 0;
}

.base_portrait img {
    width: 100%;
    height: 100%;
}


/* 信息 */

.base_information {
    float: left;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.base_information_p {
    color: #666666;
    font-size: 0.24rem;
}


/* 位置和收藏数 */

.base_,
.base_address,
.Collection {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.base_address,
.Collection {
    margin-right: 0.2rem;
}


/* 位置 */

.base_address img {
    width: 0.21rem;
    height: 0.26rem;
    margin-right: 0.1rem;
}


/* 收藏 */

.Collection img {
    width: 0.26rem;
    height: 0.22rem;
    margin-right: 0.1rem;
}

.septum_p {
    margin: 0.15rem 0rem;
}


/* 关注/收藏 */

.base_follow {
    float: right;
    color: #ffffff;
    font-size: 0.28rem;
    text-align: center;
    height: 0.55rem;
    line-height: 0.55rem;
    padding: 0rem 0.25rem;
    background: #82a541;
    border-radius: 5px;
    overflow: hidden;
}

.base_follow_cancel {
    float: right;
    color: #ffffff;
    font-size: 0.28rem;
    text-align: center;
    height: 0.55rem;
    line-height: 0.55rem;
    padding: 0rem 0.25rem;
    background: #b96210;
    border-radius: 5px;
    overflow: hidden;
    display: none;
}


/* 基地介绍 */

.base_introduce {
    width: 100%;
    padding: 0.35rem 0rem 0.7rem 0rem;
    box-sizing: border-box;
    position: relative;
}

.base_introduce_title {
    color: #666666;
    font-size: 0.24rem;
    margin-bottom: 0.1rem;
}

.base_introduce_pre {
    color: #666666;
    font-size: 0.24rem;
    padding: 0rem 0.3rem;
    box-sizing: border-box;
    text-align: justify;
    white-space: pre-wrap;
    overflow: hidden;
    line-height: 0.35rem;
}


/* 默认状态 */

.base_introduce_pre_default {
    max-height: 3.6rem;
}


/* 下拉显示全部 */

.down_display {
    width: 100%;
    height: 0.7rem;
    position: absolute;
    bottom: 0;
    left: 0;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.down_display img {
    width: 0.28rem;
    height: 0.11rem;
}


/* 暂无数据提示 */

.NoMatchData {
    text-align: center;
    font-size: 0.3rem;
    color: #ed2018;
}

/* 灰色 没有更多了 */
.gray_baseline{
    background: #f5f4f7 !important;
}

/* 白色 */
.white_baseline{
    background: #fff;
}



/* 过渡开始状态 */
.v-enter{
    transform: translate(100%,0);
    opacity: 0;
}
/* 过渡进入状态 */
.v-enter-active{
    transition:0.3s;
    transform: translate(100%,0);
}
/* 过渡结束状态 */
.v-enter-to{
    opacity:1;
    transform: translate(0%,0);
}



  